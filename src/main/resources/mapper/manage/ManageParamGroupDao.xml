<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="io.renren.modules.manage.dao.ManageParamGroupDao">

    <!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="io.renren.modules.manage.entity.ManageParamGroupEntity" id="manageParamGroupMap">
        <result property="groupId" column="group_id"/>
        <result property="groupName" column="group_name"/>
    </resultMap>
    <select id="getIdByName" resultType="java.lang.Long">
        select group_id
        from manage_param_group
        where group_name = #{groupName,jdbcType=VARCHAR}
    </select>
    <select id="getParamIdByName" resultType="java.lang.Long">
        select param_id
        from manage_param
        where param_name = #{paramName,jdbcType=VARCHAR}
    </select>


</mapper>
